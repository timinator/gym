<div id='new_workout' style='border-bottom: 1px black dashed; padding: 8px;'>
  <%= form_tag '/workouts', :remote => true do %>
    <%= label_tag :start_time %>
    <%= text_field_tag :start_time %>
    <%= label_tag :end_time %>
    <%= text_field_tag :end_time %>
    <%= submit_tag("Save") %>
  <% end %>
</div>